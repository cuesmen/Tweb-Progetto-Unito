        ],
        "includePattern": ".+\\.js(doc|x)?$"
    },
    "opts": {
        "destination": "docs",
        "recurse": true,
        "template": "templates/default"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
}